    "@mantine/core": "^5.10.3",
    "@mantine/form": "^5.10.3",
    "@mantine/hooks": "^5.10.3",
    "@mantine/modals": "^5.10.3",
    "@mantine/notifications": "^5.10.3",
    "@mantine/spotlight": "^5.10.3",
    "@mantine/styles": "^5.10.3",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.3.2",
    "dayjs": "^1.11.7",
    "interweave": "^13.0.0",
    "lodash": "^4.17.21",
    "make-plural": "^7.2.0",
    "mousetrap": "^1.6.5",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-contexify": "^6.0.0",
