        "@mantine/modals": "^5.1.5",
        "@mantine/notifications": "^5.1.5",
        "@mantine/spotlight": "^5.1.5",
        "@reduxjs/toolkit": "^1.8.4",
        "axios": "^0.27.2",
        "dayjs": "^1.11.5",
        "make-plural": "^7.1.0",
        "mousetrap": "^1.6.5",
        "react": "^18.2.0",
