{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "customManagers:biomeVersions",
    ":automergeMinor",
    ":automergePr",
    ":automergeRequireAllStatusChecks",
    ":automergeBranch",
    ":maintainLockFilesWeekly"
  ],
  "packageRules": [
    {
      "matchManagers": "maven",
      "matchPackagePatterns": "commafeed-client",
      "enabled": false
    },
    {
      "matchManagers": "npm",
      "rangeStrategy": "bump"
    }
  ]
}
